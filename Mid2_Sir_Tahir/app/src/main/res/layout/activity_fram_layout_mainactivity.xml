<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#03A9F4"
    tools:context=".StaticFragmentExample.FrameLayoutExample.FramLayoutMainactivity">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="612dp"
        android:layout_height="138dp"
        android:text="Main Activity"
        android:textSize="60sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnFragment1"
        android:layout_width="171dp"
        android:layout_height="69dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="12dp"
        android:text="First Fragment"
        android:textSize="16sp"
        app:iconSize="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/btnFragment2"
        android:layout_width="177dp"
        android:layout_height="75dp"
        android:layout_marginTop="12dp"
        android:text="Second Fragment"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnFragment1"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <FrameLayout
        android:id="@+id/mainframLayout"
        android:layout_width="771dp"
        android:layout_height="755dp"
        android:layout_marginTop="76dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnFragment1">

    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>